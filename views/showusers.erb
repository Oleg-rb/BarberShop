<h1> База данных клиентов </h1>

<%= @results %>

<table class="table">
	<% @results.each do |row| %>
		<tr>
			<td><%= row['id'] %></td>
			<td><%= row['name'] %></td>
			<td><%= row['phone'] %></td>
			<td><%= row['date_stamp'] %></td>
			<td><%= row['barber'] %></td>
			<td><%= row['color'] %></td>
		</tr>
	<% end %>
</table>
